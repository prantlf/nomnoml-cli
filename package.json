  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "grunt": "^1.4.1",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-nodeunit": "^4.0.0",
    "grunt-eslint": "^24.0.0",
    "grunt-istanbul": "^0.8.0",
    "grunt-shell": "^3.0.1",
    "load-grunt-tasks": "^5.1.0",
    "time-grunt": "^2.0.0"
